{
  "name": "gitstart-greatepull",
  "description": "A GitHub action for executing a simple script to create a PR",
  "version": "1.0.0",
  "author": "GitStart",
  "license": "MIT",
  "main": "dist/index.js",
  "private": true,
  "scripts": {
    "build": "ncc build src/main.ts",
    "format:check": "prettier --check src",
    "format:write": "prettier --write src",
    "lint": "eslint src",
    "style:check": "run-p --continue-on-error --aggregate-output format:check lint",
    "style:write": "run-p --continue-on-error --aggregate-output format:write lint",
    "pre-commit": "run-s style:write build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pre-commit && git add dist/"
    }
  },
  "dependencies": {
    "@actions/core": "^1.2.6",
    "@actions/github": "^4.0.0",
    "@actions/io": "^1.0.2",
    "@octokit/core": "^3.2.1",
    "@octokit/plugin-paginate-rest": "^2.6.0",
    "@octokit/plugin-rest-endpoint-methods": "4.2.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.10",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "@vercel/ncc": "^0.23.0",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "husky": "^4.2.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "ts-jest": "^26.2.0",
    "typescript": "^4.0.2"
  }
}
