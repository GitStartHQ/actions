{
  "name": "gitstart-actions",
  "description": "",
  "version": "1.0.0",
  "author": "GitStart",
  "license": "MIT",
  "main": "dist/index.js",
  "private": true,
  "scripts": {
    "build:create-pull": "ncc build CreatePull/src/main.ts -o CreatePull/dist",
    "build:sync-issues": "ncc build SyncIssues/src/main.ts -o SyncIssues/dist",
    "build:ecr": "ncc build ecr/src/main.ts -o ecr/dist",
    "build": "npm run build:create-pull && npm run build:sync-issues",
    "format:check": "prettier --check **/*.ts",
    "format:write": "prettier --write **/*.ts",
    "lint": "eslint **/*.ts",
    "style:check": "run-p --continue-on-error --aggregate-output format:check lint",
    "style:write": "run-p --continue-on-error --aggregate-output format:write lint",
    "pre-commit": "run-s style:write build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pre-commit && git add ."
    }
  },
  "dependencies": {
    "@actions/core": "^1.2.6",
    "@actions/github": "^4.0.0",
    "@actions/io": "^1.0.2",
    "@octokit/core": "^3.2.1",
    "@octokit/plugin-paginate-rest": "^2.6.0",
    "@octokit/plugin-rest-endpoint-methods": "4.2.1",
    "axios": "^0.21.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.10",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "@vercel/ncc": "^0.23.0",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "husky": "^4.2.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "ts-jest": "^26.2.0",
    "typescript": "^4.0.2"
  }
}
